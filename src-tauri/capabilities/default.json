{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:window:allow-start-dragging",
    "opener:default",
    "dialog:default",
    "os:default",
    "updater:default",
    "process:allow-restart",
    "store:default"
  ]
}
